@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Entrants>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
    var entrants = Model.Content;
}

@{ Html.RenderPartial("~/Views/Partials/PageHeader.cshtml");}

<section id="entrants" class="section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                @if (!string.IsNullOrWhiteSpace(entrants.Heading))
                {
                    <h3 class="wow fadeInUp" data-wow-delay="0s">@entrants.Heading</h3>
                }

                @entrants.MainContent

                <div class="table-responsive">
                    <table class="table table-striped table-condensed">
                        <thead>
                            <tr>                         
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Estimated Swim Time (400m)</th>
                                <th>Event entered</th>                           
                            </tr>
                        </thead>
                        <tbody>
                            <tr>                 
                                <td>Tyrone</td>
                                <td>DA SILVA</td>
                                <td>20:45</td>                           
                                <td>Triathlon</td>
                            </tr>
                            <tr>
                                <td>Cormac</td>
                                <td>DEVERY</td>
                                <td>20:00</td>                           
                                <td>Triathlon</td>
                            </tr>
                            <tr>
                   
                                <td>Ruth</td>
                                <td>SCHOFIELD</td>
                                <td>16:00</td>                      
                                <td>Aquabike</td>
                            </tr>
                            <tr>                             
                                <td>Rob</td>
                                <td>BOOKHAM</td>
                                <td>16:00</td>                       
                                <td>Aquabike</td>
                            </tr>                       
                        </tbody>
                    </table>
                </div>
                <p>Total entrants: 999</p>
            </div>
        </div>
    </div>
</section>