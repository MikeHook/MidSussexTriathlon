@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.GenericContent>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
    var content = Model.Content;
}

@Html.Partial("~/Views/Partials/PageHeader.cshtml")

<!-- About Section Start -->
<section id="about" class="section-small">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                @if (!string.IsNullOrWhiteSpace(content.Heading))
                {
                    <h3 class="wow fadeInUp" data-wow-delay="0s">@content.Heading</h3>                    
                }
                @if (!string.IsNullOrWhiteSpace(content.SubHeading))
                {
                    <h4 class="wow fadeInUp" data-wow-delay="0s">@content.SubHeading</h4>
                }
                <div class="main-content">
                    @content.MainContent
                </div>
            </div>
        </div>
    </div>
</section>
<!-- About Section End -->
